#1: Basic example
runApp(list(
ui = fluidPage(
fluidRow(
column(6, offset = 3,
hr(),
h2('tinyMCE Editor:'),
tinyMCE('editor1', 'Click to edit text'),
hr(),
h2('Editor Content:'),
verbatimTextOutput('editor1_content')
)
)
),
server = function(input, output, session) {
output$editor1_content <- renderPrint({input$editor1})
}
))
load_all(); document(); install()
